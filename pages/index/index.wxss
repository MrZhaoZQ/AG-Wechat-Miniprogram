  word-wrap: break-word;
}

.specBtn{
  width: 282rpx; height: 70rpx;
  border-radius: 36rpx;
  background: #58ad4c;
  position: fixed; left: 234rpx; bottom: 66rpx;
  z-index: 66;
  text-align: center;
  line-height: 70rpx;
  font-size: 28rpx;
  color: #fff;
}

.csBtn{
  width: 127rpx; height: 127rpx;
  background: url(https://athleticgreens.fugumobile.cn/imgs/service.png) no-repeat;
  background-size: 100% 100%;
  position: fixed; right: 0; bottom: 300rpx;
  z-index: 66;
}

.shopCart{
  width: 127rpx; height: 127rpx;
  position: fixed; right: 0rpx; bottom: 600rpx;
  z-index: 66;
}
.shopCart .numIcon{
  width: 32rpx; height: 32rpx;
  background: #58ad4c;
  border-radius: 50%;
  position: absolute; right: 20rpx; top: 20rpx;
  text-align: center;
  line-height: 32rpx;
  font-size: 20rpx;
  color: #fff;
}
.shopCart image{
  width: 127rpx; height: 127rpx;
}